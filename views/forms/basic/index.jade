extends ../../layouts/layout

block styles
	link(rel="stylesheet",href="/js/vendor/pixelmatrix-uniform/themes/default/css/uniform.default.min.css",media="screen")
	link(rel="stylesheet",href="/js/vendor/chosen/chosen.css",media="screen")
	link(rel="stylesheet",href="/js/vendor/iCheckbox/css/iCheckbox.css",media="screen")
	link(rel="stylesheet",href="/js/vendor/inputlimiter/jquery.inputlimiter.1.0.css",media="screen")

block sidebar
	include ../layouts/sidebar

block content
	.content-header
		.content-header-title
			h2 Tabs
			span.description A single example shown as it might be displayed across multiple pages.

	.row-fluid
		.span12
			form.form-horizontal(action='#')
				legend Simple input fields
				.row-fluid
					.span4.form-control-label
						label(for='text') Input type text
					.span8.form-controls
						label
							input#text(type="text",value="some text value...")
							span.description.help-inline Some explanatory text here.

				.row-fluid
					.span4.form-control-label
						label(for='password') Input type password
					.form-controls.span8
						input#password(type="password",value="123123123")

				.row-fluid
					.span4.form-control-label
						label(for='readonly') Readonly input
					.form-controls.span8
						input#readonly(type="text",readonly="readonly",value="readonly input field...")

				.row-fluid
					.span4.form-control-label
						label(for='disabled') Disabled input
					.form-controls.span8
						label
							input#disabled.span12(type="text",disabled="disabled",value="disabled input field...")
							span.description.help-block Some explanatory text here.

				.row-fluid
					.span4.form-control-label
						label(for='placeholder') Placeholder
					.form-controls.span8
						input#placeholder.span12(type="text",placeholder="Placeholder...")

				.row-fluid
					.span4.form-control-label
						label(for='placeholder') Tooltip
					.form-controls.span8
						input#placeholder.span12(type="text",placeholder="Hover to see tooltip...", title="Some help here").tip

				.row-fluid
					.span4.form-control-label
						label(for='icons')
							i.icon-picture.field-icon
							| Icons
					.form-controls.span8
						input#icons(type="text")

				.row-fluid
					.span4.form-control-label
						label(for='placeholder') Spinner
					.form-controls.span8
						input#placeholder.spinner.input-mini(type="text",value="5")

				.row-fluid.warning
					.span4.form-control-label
						label Input with warning
					.form-controls.span8
						input#icons(type="text")
						span.help-inline.description Something may have gone wrong

				.row-fluid.error
					.span4.form-control-label
						label Input with error
					.form-controls.span8
						input#icons(type="text")
						span.help-inline.description Please correct the error

				.row-fluid.info
					.span4.form-control-label
						label Input with info
					.form-controls.span8
						input#icons(type="text")
						span.help-inline.description Username is already taken

				.row-fluid.success
					.span4.form-control-label
						label Input with success
					.form-controls.span8
						input#icons(type="text")
						span.help-inline.description Woohoo!

				.row-fluid
					.span4.form-control-label
						label Prepended content
					.form-controls.span8
						.input-prepend
							span.add-on @
							input(type="text",placeholder="Username")

				.row-fluid
					.span4.form-control-label
						label Appended content
					.form-controls.span8
						.input-append
							input(type="text")
							span.add-on .00

				.row-fluid
					.span4.form-control-label
						label Combined
					.form-controls.span8
						.input-prepend.input-append
							span.add-on $
							input.span10(type="text")
							span.add-on .00

				.row-fluid.form-actions
					.span4
					.form-controls.span8
						button.btn.btn-primary(type="submit") Save changes
						button.btn.btn-danger(type="button") Cancel

